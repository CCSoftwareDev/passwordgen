import time


print("Welcome to The Password Generator! Please Answer the following Questions.")
time.sleep(2)
firstQ = (input("What is your name or a family members name?"))
time.sleep(2)
secondQ = (input("What is your favourite Number(It Must Be 2 Digits only)"))
time.sleep(2)
thirdQ = input("What is your Favorite type of puncuation? Remember Dont put numbers!")

password =firstQ+ secondQ + thirdQ * 2
time.sleep(2)
print(password)
